{
  "name": "run_programmatically",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "ng lint",
    "test": "mocha --reporter mochawesome --recursive --experimental-json-modules './tests/final/**/*spec.js'",
    "test:e2e": " env TS_NODE_PROJECT=\"./e2e/tsconfig.json\"  ts-node ./e2e/runner.ts",
    "prettier": "prettier \"/**/*.ts\"  --write"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^8.1.3"
  },
  "devDependencies": {
    "@wdio/cli": "^6.5.0",
    "@wdio/local-runner": "^6.5.0",
    "@wdio/mocha-framework": "^6.5.0",
    "@wdio/spec-reporter": "^6.4.7",
    "@wdio/sync": "^6.5.0",
    "chromedriver": "^85.0.1",
    "mochawesome": "^6.1.1",
    "wdio-chromedriver-service": "^6.0.4"
  }
}
