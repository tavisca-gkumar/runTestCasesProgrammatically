{
  "name": "run_programmatically",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "lint": "ng lint",
    "test": "mocha --R spec --recursive --experimental-json-modules './tests/final/**/*spec.js'",
    "test:e2e": " env TS_NODE_PROJECT=\"./e2e/tsconfig.json\"  ts-node ./e2e/runner.ts",
    "prettier": "prettier \"/**/*.ts\"  --write"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^8.1.3"
  }
}
